    "@balena/sbvr-types": "^7.0.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/lib-storage": "^3.556.0",
    "@aws-sdk/s3-request-presigner": "^3.556.0",
    "memoizee": "^0.4.15",
    "typed-error": "^3.2.2"
  },
